	email = curtisbabnik@gmail.com
[color]
	ui = true
[pull]
	rebase = true
[credential]
	helper = cache --timeout=3600
[core]
	autocrlf = input
        editor = vim
	excludesfile = ~/.gitignore
[alias]
	co = checkout
	br = branch
	s = status -s
	amend = commit -a --amend
	lg = log --oneline --decorate --graph --all
	c = commit
